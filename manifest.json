{
	"manifest_version": 3,
	"name": "Cow Sheep Pig",
	"description": "Disable Content Security Policy rules for a specific tab",
	"version": "1.0",
	"permissions": [
		"declarativeNetRequest"
	],
	"host_permissions": [
		"http://*/*",
		"https://*/*"
	],
	"icons": {
		"128": "assets/logo.png"
	},
	"background": {
		"service_worker": "src/background.ts",
		"type": "module"
	},
	"action": {
		"default_icon": {
			"128": "assets/logo.png"
		}
	}
}